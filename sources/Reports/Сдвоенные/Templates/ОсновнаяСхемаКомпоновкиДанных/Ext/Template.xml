			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ЛЕВ(СТРОКА(Расписание.Период), 10) КАК Дата,
	Расписание.Группа КАК Группа,
	Расписание.Предмет КАК Предмет
ИЗ
	РегистрСведений.Расписание КАК Расписание
ГДЕ
	Расписание.Период МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И Расписание.ЗанятиеПроведено = ИСТИНА
	И Расписание.Совмещение = ИСТИНА
	И Расписание.Преподаватель = &amp;Преподаватель</query>
	</dataSet>
